{
    "name": "simplecomplex/config",
    "type": "library",
    "description": "Simple Cache like configuration interfaces; ini-files as source, cache as store.",
    "keywords": ["configuration", "PSR-16", "simple cache", "PHP"],
    "homepage": "https://github.com/simplecomplex/php-config",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Friis Mathiasen",
            "email": "jacob.friis@simplecomplex.net"
        }
    ],
    "require": {
        "php": ">=7.0",
        "simplecomplex/utils": "^1.6.1 || dev-develop",
        "simplecomplex/cache": "^1.1 || dev-develop"
    },
    "suggest": {
        "simplecomplex/inspect": "CLI features do NOT work without Inspect. And great for logging; better variable dumps and traces."
    },
    "autoload": {
        "psr-4": {
            "SimpleComplex\\Config\\": "src/"
        }
    }
}
